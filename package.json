{
    "name": "flagsmith-vue",
    "version": "2.0.0",
    "description": "Flagsmith Vue.js integration",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "watch": "tsc --watch",
        "build": "tsc --project tsconfig.build.json && npm run minify",
        "minify": "terser dist/index.js -o dist/index.js --ecma 2016",
        "lint": "eslint .",
        "format": "prettier . --write",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "lint-staged": {
        "*.{js,json,ts}": "prettier --write",
        "*.{js,ts}": "eslint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jhoermann/flagsmith-vue.git"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "flagsmith",
        "feature flags",
        "vue"
    ],
    "author": "Jochen HÃ¶rmann",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jhoermann/flagsmith-vue/issues"
    },
    "homepage": "https://github.com/jhoermann/flagsmith-vue#readme",
    "devDependencies": {
        "@commitlint/config-conventional": "^19.5.0",
        "@tsconfig/strictest": "^2.0.5",
        "@types/jest": "^29.5.13",
        "@vue/test-utils": "^2.4.6",
        "eslint": "^9.12.0",
        "flagsmith": "^5.0.0",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.2.10",
        "prettier": "3.3.3",
        "terser": "^5.36.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.9.0",
        "vue": "^3.5.12"
    },
    "peerDependencies": {
        "flagsmith": "^5",
        "vue": "^2.7 || ^3"
    }
}
